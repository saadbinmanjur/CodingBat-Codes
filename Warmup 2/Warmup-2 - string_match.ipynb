    "def string_match(a, b):\n",
    "  shorter = min(len(a), len(b))\n",
    "  count = 0\n",
    "  \n",
    "  for i in range(shorter-1):\n",
    "    a_sub = a[i:i+2]\n",
    "    b_sub = b[i:i+2]\n",
    "    if a_sub == b_sub:\n",
    "      count = count + 1\n",
    "  return count"
   ]
  }
